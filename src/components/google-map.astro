---
interface Props {
  mapUrl: string;
  mapTitle: string;
  loading: "eager" | "lazy";
}

const { mapUrl, mapTitle, loading = "lazy" }: Props = Astro.props;
---

<iframe
  src={mapUrl}
  class="w-full h-96 rounded-lg border-0"
  allowfullscreen="true"
  loading={loading}
  title={mapTitle}
  referrerpolicy="no-referrer-when-downgrade"
>
</iframe>
